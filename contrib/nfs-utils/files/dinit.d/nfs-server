# nfs-server service

type           = scripted
depends-on     = local.target
depends-on     = nfs-mountd
depends-on     = proc-fs-nfsd
depends-on     = rpcbind
waits-for      = nfsdcld
waits-for      = nfs-idmapd
waits-for      = rpc-statd
after          = network.target
after          = rpc-gssd
after          = rpc-svcgssd
command        = /usr/libexec/dinit-nfs-server start
stop-command   = /usr/libexec/dinit-nfs-server stop
